{
  "name": "cursorrulecraft",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "nx dev frontend",
    "dev:frontend": "nx dev frontend",
    "dev:backend": "nx dev backend",
    "dev:all": "nx run-many --targets=dev --parallel=2",
    "build": "nx run-many --targets=build",
    "build:frontend": "nx build frontend",
    "build:frontend:production": "nx build frontend --configuration=production",
    "build:backend": "nx build backend",
    "build:backend:production": "nx build backend --configuration=production",
    "build:production": "nx run-many --targets=build --configuration=production",
    "start": "nx start backend",
    "start:production": "nx start backend",
    "preview": "nx preview frontend",
    "preview:production": "nx preview frontend",
    "lint": "nx run-many --targets=lint --force --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:imports": "prettier --write \"**/*.{ts,tsx,js,jsx}\" --plugin-search-dir .",
    "lint:fix": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "type-check": "nx run-many --targets=type-check",
    "lingui:extract": "nx lingui:extract frontend",
    "lingui:compile": "nx lingui:compile frontend",
    "test": "nx run-many --targets=test",
    "graph": "nx graph",
    "affected": "nx affected",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules libs/*/node_modules",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:studio": "open http://127.0.0.1:54323",
    "setup": "bun install && echo '\n⚠️  Next step: Install Supabase CLI if you haven''t already:\n   macOS: brew install supabase/tap/supabase\n   npm:   npm install -g supabase\n\nThen run: supabase start\nCopy the credentials to .env file, and run: bun run dev:all'"
  },
